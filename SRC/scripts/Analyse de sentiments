import pandas as pd
from setfit import SetFitModel
from transformers import pipeline


# Chemin vers le fichier t√©l√©charg√©
file_path = 'CDE_Satisfaction2023/data/processed/L5_processed.csv'

# Charger le fichier dans un DataFrame
df = pd.read_csv(file_path)

# Afficher les premi√®res lignes pour inspection
print(df.head())


# Download from Hub and run inference
model = SetFitModel.from_pretrained("/fprencipe3/setfit-TrustPilot")
# Run inference
preds = model(["i loved the spiderman movie!", "pineapple on pizza is the worst ü§Æ"])